---
// Component Imports
import Bouton from "../components/Bouton";
import Layout from "../layouts/default.astro";
import "../styles/index.css";

// Full Astro Component Syntax:
// https://docs.astro.build/core-concepts/astro-components/
---

<Layout>
  <main class="content">
    <div class="home">
      <div class="home_txt">
        <div class="title">
          <h1>ETOILE</h1>
          <h1>GOUSSAINVILLOISE</h1>
        </div>
        <div class="text">
          <p class="textpad">
            Club affilié à la Fédération Française de Gymnastique.
          </p>
          <p>La Gymnastique permet de développer force,</p>
          <p>souplesse, grâce, ainsi qu’une très bonne</p>
          <p>coordination des mouvements.</p>
        </div>
        <Bouton />
      </div>
      <div class="home_logo">
        <img src="/assets/logo.svg" alt="Gym logo" class="python_svg" />
      </div>
    </div>
  </main>
</Layout>
